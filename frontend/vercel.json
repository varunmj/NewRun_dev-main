{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "rewrites": [
      { "source": "/api/:path*", "destination": "https://api.newrun.club/:path*" },
      { "source": "/socket.io/:path*", "destination": "https://api.newrun.club/socket.io/:path*" },
      { "source": "/((?!.*\\..*).*)", "destination": "/" }
    ]
  }
  